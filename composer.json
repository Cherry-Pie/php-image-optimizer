{
    "name": "lciolecki/php-image-optimizer",
    "author": "Lukasz Ciolecki",
    "description": "PHP image file optimizer (uses https://github.com/bensquire/php-image-optim)",
    "type": "library",
    "keywords": [
        "php",
        "image",
        "optimizer"
    ],
    "license": "BSD-3-Clause",
    
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bensquire/php-image-optim"
        }
    ],
    
    "require": {
        "php": ">=5.4",
        "bensquire/php-image-optim": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "Extlib": "src/",
            "ExtlibTest": "tests/"
        }
    }
}
